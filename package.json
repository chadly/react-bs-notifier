{
  "name": "react-bs-notifier",
  "version": "0.4.1",
  "description": "A react component to show growl-like notifications using bootstrap alerts",
  "keywords": [
    "react-component",
    "react",
    "growl",
    "notifier",
    "notifications",
    "bootstrap",
    "alerts"
  ],
  "main": "alerts.js",
  "author": "Chad Lee <npm@chadly.net>",
  "license": "MIT",
  "scripts": {
    "build": "babel alerts.jsx > alerts.js",
    "test": "jshint . --exclude node_modules",
    "prepublish": "npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadly/react-bs-notifier"
  },
  "bugs": {
    "url": "https://github.com/chadly/react-bs-notifier/issues"
  },
  "browserify": {
    "transform": [
      "node-lessify"
    ]
  },
  "dependencies": {
    "node-lessify": "0.0.10"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-addons-css-transition-group": ">=0.14"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "jshint": "^2.7.0"
  }
}
